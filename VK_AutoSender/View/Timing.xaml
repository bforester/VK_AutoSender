<Window x:Class="VK_AutoSender.View.Timing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VK_AutoSender.View" xmlns:wpfx="http://schemas.xceed.com/wpf/xaml/toolkit"
        Icon="../Resources/icon.ico"
        mc:Ignorable="d"
        Title="Timing" Height="120" Width="400" ResizeMode="NoResize">
    <StackPanel Orientation="Horizontal" Background="#FF3C3C3C">
        <wpfx:DateTimePicker x:Name="DateTimePicker" Height="52" TextAlignment="Center" FontSize="15" HorizontalAlignment="Left" Margin="7,0,0,0" VerticalAlignment="Center" Width="270" BorderBrush="#FF4F6ABD"/>
        <Button Click="OkTiming" Content="Ok" Margin="10" Width="100" Height="50" FontSize="30" Background="#FF44DEAA"/>
       
    </StackPanel>
</Window>
